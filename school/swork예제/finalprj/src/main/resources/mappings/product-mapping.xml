<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ProductDAO">

	<resultMap type="productDTO" id="productList">
		<id property="pd_seq" column="PD_SEQ" />
		<result property="pd_name" column="PD_NAME" />
	</resultMap>

	<select id="selectProductAll" resultMap="productList">
		SELECT * FROM PRODUCT
	</select>

<!-- 	<select id="getSomething" resultType="productDTO"> -->
	
<!-- 	</select> -->

	<insert id="insertProduct">
		INSERT INTO PRODUCT (PD_NAME) VALUES (#{pd_name})
	</insert>

	<update id="updateSomething">
	
	</update>

	<delete id="deleteSomething">
	
	</delete>

</mapper>
